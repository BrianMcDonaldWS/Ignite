{
  "name": "@fuego/ignite",
  "version": "0.0.55",
  "description": "Modern markdown documentation generator",
  "main": "dist/ignite.js",
  "bin": {
    "ignite": "./dist/ignite-cli.js"
  },
  "files": [
    "dist",
    "src",
    "webpack.config.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.intuit.com/Fuego/Ignite.git"
  },
  "author": {
    "name": "Andrew Lisowski",
    "email": "andrew_lisowski@intuit.com"
  },
  "scripts": {
    "precommit": "lint-staged",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "xo",
    "lint:css": "stylelint 'src/**/*.css'",
    "test": "jest",
    "serve": "webpack-serve",
    "start": "npm run build -- -w",
    "build": "chmod 777 ./dist/ignite-cli.js && ./dist/ignite-cli.js",
    "build:lib": "babel src -d dist --ignore '/__tests__/'",
    "build:lib:watch": "npm run build:lib -- -w --verbose",
    "publish:docs": "npm run build -- --publish",
    "analyze": "npm run build -- --analyze"
  },
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "7.0.0-beta.51",
    "@babel/core": "7.0.0-beta.51",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.51",
    "@babel/plugin-transform-react-constant-elements": "7.0.0-beta.51",
    "@babel/plugin-transform-react-inline-elements": "7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-react": "7.0.0-beta.51",
    "@babel/preset-stage-3": "^7.0.0-beta.51",
    "@babel/register": "7.0.0-beta.51",
    "@reach/router": "1.1.1",
    "ast-types": "0.11.5",
    "autoprefixer": "^8.6.2",
    "babel-loader": "^8.0.0-beta",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "bulma-checkradio": "1.0.2",
    "cheerio": "1.0.0-rc.2",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "0.1.19",
    "connect-history-api-fallback": "1.5.0",
    "copy-webpack-plugin": "4.5.1",
    "cosmiconfig": "5.0.5",
    "css-loader": "^0.28.11",
    "dayjs": "1.6.7",
    "escodegen": "1.10.0",
    "esprima": "4.0.0",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "get-line-from-pos": "1.0.0",
    "gh-pages": "^1.2.0",
    "globby": "^8.0.1",
    "gravatar": "1.6.0",
    "highlight.js": "^9.12.0",
    "html-webpack-plugin": "^3.2.0",
    "js-worker-search": "1.2.1",
    "js-yaml": "3.12.0",
    "koa-connect": "2.0.1",
    "loader-utils": "^1.1.0",
    "lqip-loader": "2.1.1",
    "markdown-it": "^8.4.1",
    "markdown-it-anchor": "^4.0.0",
    "markdown-it-attrs": "2.2.0",
    "markdown-it-br": "^1.0.0",
    "markdown-it-checkbox": "^1.1.0",
    "markdown-it-container": "^2.0.0",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-external-links": "0.0.6",
    "markdown-it-front-matter": "git+https://github.com/hipstersmoothie/markdown-it-front-matter.git#use-callback",
    "markdown-it-highlight-lines": "^1.0.1",
    "markdown-it-ins": "^2.0.0",
    "markdown-it-jsx": "1.1.0",
    "markdown-it-mark": "^2.0.0",
    "markdown-it-regexp": "^0.4.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-table-of-contents": "0.3.3",
    "mini-css-extract-plugin": "^0.4.0",
    "postcss-color-rgba-fallback": "^3.0.0",
    "postcss-initial": "^2.0.0",
    "postcss-loader": "^2.1.5",
    "postcss-nested": "^3.0.0",
    "postcss-pseudoelements": "^5.0.0",
    "postcss-simple-vars": "^4.1.0",
    "probe-image-size": "4.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.1",
    "react-color": "2.14.1",
    "react-dom": "^16.4.1",
    "react-gist": "1.2.1",
    "react-ideal-image": "0.0.5",
    "react-router-prop-types": "^1.0.3",
    "react-snap": "1.13.0",
    "react-tweet-embed": "1.1.0",
    "react-waypoint": "8.0.1",
    "request-promise": "4.2.2",
    "root-path": "^0.2.1",
    "simple-git": "1.96.0",
    "throttle-debounce": "2.0.0",
    "transform-markdown-links": "^2.0.0",
    "uglifyjs-webpack-plugin": "1.2.5",
    "webpack": "^4.12.0",
    "webpack-inject-plugin": "~0.4.0",
    "webpack-serve": "1.0.4",
    "webpack-serve-waitpage": "1.0.0",
    "zeedown": "2.0.1"
  },
  "devDependencies": {
    "@fuego/github-release": "^2.1.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-xo-react": "^0.16.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "jest-css-modules": "^1.1.0",
    "jest-junit": "^5.0.0",
    "jest-watch-typeahead": "0.1.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.5",
    "react-test-renderer": "^16.4.1",
    "stylelint": "^9.2.1",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-csstree-validator": "1.3.0",
    "stylelint-scss": "^3.1.0",
    "webpack-bundle-analyzer": "2.13.1",
    "xo": "^0.21.1",
    "yargs": "^11.0.0"
  },
  "prettier": {
    "singleQuote": true
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "xo": {
    "parser": "babel-eslint",
    "extends": [
      "prettier",
      "xo-react/space"
    ],
    "plugins": [
      "prettier",
      "jest"
    ],
    "env": [
      "browser",
      "node",
      "jest"
    ],
    "rules": {
      "no-console": [
        2,
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "max-params": [
        1,
        5
      ],
      "react/jsx-tag-spacing": {
        "closingSlash": "never",
        "beforeSelfClosing": "always",
        "afterOpening": "never"
      }
    }
  },
  "jest": {
    "transform": {
      "\\.(css)$": "jest-css-modules",
      "^.+\\.js?$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "/dist/",
      "/node_modules/",
      "/__tests__/utils/",
      "/testExtension/"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/testExtension/*.js"
    ],
    "coverageReporters": [
      "cobertura",
      "html",
      "lcov"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "jest-junit": {
    "output": "./coverage/junit/junit.xml"
  },
  "reactSnap": {
    "source": "_ignite"
  }
}
